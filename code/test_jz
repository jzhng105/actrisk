import pandas as pd
import numpy as np
import multiprocessing as mp
from code.utils.utils import parallel_calculation, calculate

@parallel_calculation(pool_size=4)
def calculate(df):
    df[2] = df[0]*2
    return df

if __name__ == "__main__":
    data = np.transpose(pd.DataFrame([range(1,101), range(1,101)]))
    
    results = calculate(data)
    print(results)
